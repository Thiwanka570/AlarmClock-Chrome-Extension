{
    "manifest_version": 2,
    "name": "Alarm Clock",
    "version": "1.0",
    "description": "Set Alarm ",
    "browser_action": {
      "default_popup": "index.html"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["process.js"]
        }
      ]
  }
  